<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.greencity">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logo_wtht_leaf"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo_wtht_leaf"
        android:supportsRtl="true"
        android:theme="@style/Theme.GreenCity">
        <activity android:name=".HouseholdWasteActivity"></activity>
        <activity android:name=".GlassWasteActivity" />
        <activity android:name=".PlasticWasteActivity" />
        <activity android:name=".AluminiumWasteActivity" />
        <activity android:name=".PaperWasteActivity" />
        <activity android:name=".OrganicsWasteActivity" />
        <activity
            android:name=".SearchActivity"
            android:label="@string/title_activity_information" />
        <activity
            android:name=".FullscreenActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/app_name"
            android:theme="@style/Theme.GreenCity.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>